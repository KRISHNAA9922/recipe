      "package": "com.krish23.recipe"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "recipe",
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "09558a7e-519b-41a5-81b9-26471cc033c5"
      }
    }
